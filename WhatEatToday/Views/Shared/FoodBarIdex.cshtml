<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="icon" href="@Url.Content("~/Content/")img/favicon.png" type="image/png">
    <title>About Us</title>
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="@Url.Content("~/Content/")css/bootstrap.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/")vendors/linericon/style.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/")css/font-awesome.min.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/")vendors/owl-carousel/owl.carousel.min.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/")vendors/lightbox/simpleLightbox.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/")vendors/nice-select/css/nice-select.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/")vendors/animate-css/animate.css">
    <!-- main css -->
    <link rel="stylesheet" href="@Url.Content("~/Content/")css/style.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/")css/responsive.css">
    <!--================ End footer Area  =================-->
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="@Url.Content("~/Content/")js/jquery-3.2.1.min.js"></script>
    <script src="@Url.Content("~/Content/")js/popper.js"></script>
    <script src="@Url.Content("~/Content/")js/bootstrap.min.js"></script>
    <script src="@Url.Content("~/Content/")js/stellar.js"></script>
    <script src="@Url.Content("~/Content/")vendors/lightbox/simpleLightbox.min.js"></script>
    <script src="@Url.Content("~/Content/")vendors/nice-select/js/jquery.nice-select.min.js"></script>
    <script src="@Url.Content("~/Content/")vendors/isotope/imagesloaded.pkgd.min.js"></script>
    <script src="@Url.Content("~/Content/")vendors/isotope/isotope-min.js"></script>
    <script src="@Url.Content("~/Content/")vendors/owl-carousel/owl.carousel.min.js"></script>
    <script src="@Url.Content("~/Content/")js/jquery.ajaxchimp.min.js"></script>
    <script src="@Url.Content("~/Content/")vendors/counter-up/jquery.waypoints.min.js"></script>
    <script src="@Url.Content("~/Content/")vendors/counter-up/jquery.counterup.js"></script>
    <script src="@Url.Content("~/Content/")js/mail-script.js"></script>
    <script src="@Url.Content("~/Content/")vendors/popup/jquery.magnific-popup.min.js"></script>
    <script src="@Url.Content("~/Content/")vendors/scroll/jquery.mCustomScrollbar.js"></script>
    <script src="@Url.Content("~/Content/")vendors/swiper/js/swiper.min.js"></script>
    <script src="@Url.Content("~/Content/")js/theme.js"></script>
    <style>
        #map {
            height: 400px;
            width: 400px;
            align-self: center;
        }
    </style>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBGQSBUZg2rVkQOPy-gPA9SHLRf3Z1P4Xw&callback=initMap">
    </script>

    @Scripts.Render("~/Scripts/jquery-3.3.1.js")
    @Scripts.Render("~/Content/js/chart.js")
    @Scripts.Render("~/Content/js/WhatEatToday.js")
    <title>@ViewBag.Title</title>
</head>
<body style="background-color:bisque">
    
    <!--================Header Menu Area =================-->
    <header class="header_area navbar_fixed">
        <div class="main_menu">
            <nav class="navbar navbar-expand-lg navbar-light animated lightSpeedIn" style="background-color:#EE7621">
                <div class="container">
                    <!-- Brand and toggle get grouped for better mobile display -->
                    <a class="navbar-brand logo_h" href="index.html">
                        <img class="logo-1" src="@Url.Content("~/Content/")img/logo-2.png" width="150px" alt="">
                        <img class="logo-2" src="@Url.Content("~/Content/")img/logo-2.png" alt="">
                    </a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                            aria-expanded="false" aria-label="Toggle navigation">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse offset" id="navbarSupportedContent">
                        <ul class="nav navbar-nav menu_nav ml-auto">
                            <li class="nav-item"><a class="nav-link" href="~/Home/Index/">Home</a></li>
                            <li class="nav-item"><a class="nav-link" href="~/Shops/FindShop/">Find Shop</a></li>
                            <li class="nav-item"><a class="nav-link" href="~/Game/Index/">Games</a></li>
                            <li class="nav-item"><a class="nav-link" href="#">Contact Us</a></li>
                            <li class="nav-item"><a class="nav-link" href="~/Account/Login">Login</a></li>
                            <li class="nav-item"><a class="nav-link" href="~/Account/Register">Register</a></li>
                        </ul>
                    </div>
                </div>
            </nav>
        </div>
    </header>
    <!--================Header Menu Area =================-->
   

    <div>
        @RenderBody()
    </div>
</body>


</html>
